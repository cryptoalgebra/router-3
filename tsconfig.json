{
  "extends": "./base",
  "include": ["./evm", "modules/Holesky.ts",],
  "exclude": ["**/*.test.ts", "./dist/**"],
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./dist",
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "declaration": true,
    "declarationMap": true
  }
}
